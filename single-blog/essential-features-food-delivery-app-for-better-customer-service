<div class="back">
<img class="conv1" src="/storage/app/media/conv1.svg" alt="icon mail">
<img class="conv2" src="/storage/app/media/conv2.svg" alt="icon mail">
<div class="circle1"></div>
<div class="circle2"><span onclick="unflip()">X</span></div>
<h3><div id="result">Thank you for Subscription!</div></h3>
</div>
</div>
<script>
    var card = document.getElementById('card');
    var input = document.getElementById('email');
   
     function flip() {
         if(input.value.indexOf('@') > 0 ) {
             input.classList.remove('error');
              card.classList.add('flip');
         } else {
           input.classList.add('error');
            }
        
    }
    
     function unflip() {
         card.classList.remove('flip');
        document.getElementById('email').value = "";
    }
    
</script>

