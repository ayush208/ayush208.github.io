        border-bottom: 1px solid rgba(0, 0, 0, .1);
    }
    
    .card input::placeholder {
        color: #333333;
        font-family: 'Poppins', sans-serif;
        font-style: normal;
        font-weight: 500;
        font-size: 14px;
        line-height: 32px;
    }
    
    .card input.error {
        border: 1px solid red;    
    }
