            width: 58%;
        }
        .card h3 {
             font-size: 30px; 
        }
        .card form {
            width: 100%;
        }
       .card .conv2 {
            display: none;    
        }
    }
    
    @media(max-width: 640px) {
         .back h3 {
            left: 20%;
        } 
        .card-action {
            width: 100%;
            margin-bottom: 20px;
        }
         .card-action button {
             font-size: 18px;
             line-height: 24px;
             margin-top: 5px;
             height: auto;
        } 
         .front {
            flex-wrap: wrap;    
        }
        .card-content {
            width: 100%;
        }
        .card-content img {
            max-height: 34px;
        }
        .card {
            height: auto;
        }
        .card form {
            padding-top: 25px;
            padding-right: 0;
        }
    }
    
    @media(max-width: 550px) {
        .back h3 {
