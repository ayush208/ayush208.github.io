<path fill-rule="evenodd" clip-rule="evenodd" d="M329.442 527.579C338.91 512.112 353.449 514.273 353.449 514.273C353.449 514.273 345.679 510.569 343.091 509.139C343.091 509.139 332.022 504.939 320.381 521.584" fill="white" />
<path d="M329.442 527.579C338.91 512.112 353.449 514.273 353.449 514.273C353.449 514.273 345.679 510.569 343.091 509.139C343.091 509.139 332.022 504.939 320.381 521.584" stroke="black" stroke-width="2" stroke-linejoin="round" />
<path fill-rule="evenodd" clip-rule="evenodd" d="M444.577 254.393C427.786 241.798 403.578 246.271 403.592 230.638C403.62 200.309 416.888 181.131 437.054 175.271C437.482 175.135 437.949 175.236 438.413 175.506" fill="white" />
<path d="M444.577 254.393C427.786 241.798 403.578 246.271 403.592 230.638C403.62 200.309 416.888 181.131 437.054 175.271C437.482 175.135 437.949 175.236 438.413 175.506" stroke="black" stroke-width="2" stroke-linejoin="round" />
<path fill-rule="evenodd" clip-rule="evenodd" d="M443.639 297.967C443.639 297.967 443.792 306.778 449.106 311.784C453.038 315.487 455.326 316.39 467.045 318.065C478.764 319.739 504.131 324.988 504.55 308.128" fill="white" />
<path d="M443.639 297.967C443.639 297.967 443.792 306.778 449.106 311.784C453.038 315.487 455.326 316.39 467.045 318.065C478.764 319.739 504.131 324.988 504.55 308.128" stroke="black" stroke-width="2" stroke-linejoin="round" />
