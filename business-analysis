<script>
                var a = 0;
                var oTop = $('.customers__care').offset().top - window.innerHeight;
                
                function counterStart() {
                    if (a == 0 && $(window).scrollTop() > oTop) {
                        $('.counter').each(function() {
                            var $this = $(this),
                                countTo = $this.attr('data-count');
                            $({
                                countNum: $this.text()
                            }).animate({
                                countNum: countTo
                            }, {
                                duration: 2500,
                                easing: 'swing',
                                step: function() {
                                    $this.text(Math.floor(this.countNum));
                                },
                                complete: function() {
                                    $this.text(this.countNum);
                                }
                            });
                        });
                        a = 1;
                    }
                }
                
                counterStart();
                
                $(window).scroll(function() {
                    counterStart();
                });
            </script>


