<path stroke="#000" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M33.5 132.6l26-14.7c2.2-1.2 5-1.2 7.3.1L93.5 134c2 1.1 2 4 0 5.1l-25.8 14.1a7.3 7.3 0 0 1-7 0l-27.2-15.3a3 3 0 0 1 0-5.2z" />
<path fill="#FFF" d="M63.3 161.8v9.7" />
